#include <iostream>
#include <vector>
#include <algorithm>

using namespace std;

int main() {
	

	ios_base::sync_with_stdio(false);
	cin.tie(NULL);
	cout.tie(NULL);

	int N, M;

	cin >> N;

	vector<int> card(N);

	for (int i = 0; i < N; i++)
		cin >> card[i];

	cin >> M;
	sort(card.begin(), card.end());
	
	for (int i = 0; i < M; i++) {

		int target;
		cin >> target;

		auto lower = lower_bound(card.begin(), card.end(), target);
		auto upper = upper_bound(card.begin(), card.end(), target);

		cout << upper - lower << " ";

	}

	return 0;

}
