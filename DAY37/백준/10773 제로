#include <iostream>
#include <stack>

using namespace std;

int main() {
	
	int K;
	cin >> K;
	
	stack<int> money;

	for (int i = 0; i < K; i++) {
		int num;
		cin >> num;

		if (num == 0 && !money.empty()) {
			money.pop();
		}
		else 
			money.push(num);

	}

	int budget = 0;

	while (!money.empty()) {
		budget += money.top();
		money.pop();
	}

	cout << budget;

	return 0;

}
