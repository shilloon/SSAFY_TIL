#include <iostream>
#include <vector>

using namespace std;

int main() {

	int T;
	cin >> T;

	for (int test_case = 1; test_case <= T; test_case++) {

		int N;
		cin >> N;

		vector<int> dp(N + 1);

		dp[1] = 1;
		dp[2] = 2;
		dp[3] = 4;

		for (int i = 4; i <= N; i++) {

			dp[i] = dp[i - 3] + dp[i - 2] + dp[i - 1];

		}

		cout << dp[N] << "\n";

	}

	return 0;

}
