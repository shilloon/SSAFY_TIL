#include <iostream>

using namespace std;

int main()
{

	string s;

	cin >> s;

	int index = 0;
	int sum_value = 0;

	for (int i = 0; i < s.size(); i++) {
		
		if (s[i] == '*')
			index = i;
		else {
			
			if (i % 2 == 1) {
				sum_value += ((s[i] - '0') * 3);
			}
			else {
				sum_value += (s[i] - '0');
			}
		}

	}

	int result = 0;

	if (index % 2 == 1) {
		
		for (int i = 1; i < 10; i++){
			if ((sum_value + (i * 3)) % 10 == 0){
				result = i;
				break;
			}
		}

	}
	else {

		for (int i = 1; i < 10; i++) {
			if ((sum_value + i) % 10 == 0) {
				result = i;
				break;
			}
		}

	}

	cout << result;

}
