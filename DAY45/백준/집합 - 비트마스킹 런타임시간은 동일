#include <iostream>
#include <string>

using namespace std;

int main() {

	ios_base::sync_with_stdio(false);
	cin.tie(NULL);

	int N;
	cin >> N;
	int S = 0;

	for (int i = 0; i < N; i++) {
		string command;
		cin >> command;

		if (command == "all")
			S = (1 << 21) - 1;
		else if (command == "empty")
			S = 0;
		else {
			int num;
			cin >> num;

			if (command == "add")
				S |= (1 << num);
			else if (command == "remove")
				S &= ~(1 << num);
			else if (command == "check") {
				if (S & (1 << num))
					cout << 1 << "\n";
				else
					cout << 0 << "\n";
			}
			else if (command == "toggle")
				S ^= (1 << num);
		}

	}

	return 0;

}
