#include <iostream>
#include <string>
#include <stack>

using namespace std;

int main() {
    string s;

    while (getline(cin, s)) {

        if (s.size() == 1 && s[0] == '.') {
            break;
        }

        stack<char> bracket;
        bool is_valid = true;

        for (char ch : s) {
            if (ch == '(' || ch == '[' || ch == '{') {
                bracket.push(ch);
            }
            else if (ch == ')') {
                if (bracket.empty() || bracket.top() != '(') {
                    is_valid = false;
                    break;
                }
                bracket.pop();
            }
            else if (ch == ']') {
                if (bracket.empty() || bracket.top() != '[') {
                    is_valid = false;
                    break;
                }
                bracket.pop();
            }
            else if (ch == '}') {
                if (bracket.empty() || bracket.top() != '{') {
                    is_valid = false;
                    break;
                }
                bracket.pop();
            }
        }

        if (is_valid && bracket.empty()) {
            cout << "yes" << endl;
        }
        else {
            cout << "no" << endl;
        }
    }

    return 0;
}
