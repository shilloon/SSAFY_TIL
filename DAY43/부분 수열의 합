import sys
sys.stdin = open("input.txt", "r")


def dfs(idx, value_sum):

    global count

    if value_sum > K:
        return

    if value_sum == K:
        count += 1
        return

    if idx == N:
        return

    dfs(idx+1, value_sum + arr[idx])
    dfs(idx+1, value_sum)


T = int(input())
for tc in range(1, T+1):
    N, K = map(int, input().split())

    arr = list(map(int, input().split()))
    count = 0

    dfs(0, 0)

    print(f'#{tc} {count}')
