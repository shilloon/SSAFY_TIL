#include <iostream>
#include <stack>
#include <vector>

using namespace std;

int main() {

	int N;
	cin >> N;

	stack<int> st;
	vector<char> result;
	int cur_num = 1;

	for (int i = 0; i < N; i++) {
		int target_num;
		cin >> target_num;

		while (cur_num <= target_num) {
			st.push(cur_num);
			result.push_back('+');
			cur_num++;
		}
	
		if (st.top() == target_num) {
			st.pop();
			result.push_back('-');
		}
		else {
			cout << "NO";
			return 0;
		}

	}

	for (int i = 0; i < result.size(); i++) {
		cout << result[i] << '\n';
	}

	return 0;

}
