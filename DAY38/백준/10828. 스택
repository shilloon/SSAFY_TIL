#include <iostream>
#include <stack>

using namespace std;

int main() {

	int N;
	cin >> N;
	stack<int> command;
	int is_valid;

	for (int i = 0; i < N; i++) {

		string s;
		cin >> s;
		if (s == "push") {
			int num;
			cin >> num;
			command.push(num);
		}
		else if (s == "top") {

			if (command.empty())
				cout << -1 << endl;
			else
				cout << command.top() << endl;
		}
		else if (s == "size")
			cout << command.size() << endl;
		else if (s == "empty") {
			if (command.empty())
				cout << 1 << endl;
			else
				cout << 0 << endl;
		}
		else if (s == "pop") {

			if (command.empty())
				cout << -1 << endl;
			
			else {
				int popnum = command.top();
				command.pop();
				cout << popnum << endl;
			}

		}
	}

	return 0;

}
