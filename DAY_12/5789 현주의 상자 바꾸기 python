T = int(input())

for t in range(1, T+1):

    N, Q = map(int, input().split())

    box_num = []

    # 각 박스의 자리를 0으로 초기화
    for i in range(N):
        box_num.append(0)

    # L~R까지 자리의 box_num을 +1
    for i in range(1, Q+1):

        L, R = map(int, input().split())

        for j in range(L-1, R):
            box_num[j] = i

    # fstring을 사용할 경우 unpacking이 안됌
    box = ' '.join(map(str, box_num))

    print(f'#{t} {box}')

