T = int(input())

for t in range(1, T+1):

    N = int(input())

    block = list(map(int, input().split()))

    max_count = 0

    # 각 블럭 위치에서의 낙하 위치
    for i in range(N-1):

        count = 0

        # i번째 다음 블럭과의 충돌 위치 계산
        for j in range(i + 1, N):

            # 블럭과의 충돌 계산
            if block[i] <= block[j]:
                continue
            else:
                count += 1

        # 낙하가 가장 큰 count 저장
        if max_count < count:
            max_count = count


    print(f'#{t} {max_count}')
