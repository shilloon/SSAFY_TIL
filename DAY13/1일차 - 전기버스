T = int(input())

for t in range(1, T+1):

    K, N, M = map(int, input().split())

    charge_station = list(map(int, input().split()))

    charge = 0
    distance = 0

    # 버스가 도착점 N에 도착할 때까지 반복
    while distance + K < N:

        moved = False

        # 충전소를 방문할 시
        for i in range(M-1, -1, -1):

            if distance < charge_station[i] <= distance+ K:
                charge += 1
                moved = True
                distance = charge_station[i]
                break
        
        # 충전소 방문 안할 시
        if moved == False:
            charge = 0
            break

    print(f'#{t} {charge}')

