#include <iostream>
#include <queue>
#include <vector>
#include <functional>

using namespace std;

int main() {

	ios::sync_with_stdio(false);
	cin.tie(NULL);

	int N;
	cin >> N;

	priority_queue<int, vector<int>, greater<int>> min_heap;
	for (int i = 0; i < N; i++) {

		int num;
		cin >> num;

		if (num > 0) {

			min_heap.push(num);

		}
		else {

			if (min_heap.empty()) {
				cout << "0" << "\n";
			}
			else {
				cout << min_heap.top() << "\n";
				min_heap.pop();
			}
		}
	}

	return 0;

}
