T = int(input())
for tc in range(1, T+1):
    N = int(input())
    subset = list(map(int, input().split()))
    subset_sum = 0
    avg = 0

    # 평균의 평균 구하기
    for i in range(N):
        subset_sum += subset[i]
    avg = subset_sum / N

    # 만약 나뉘어 떨어지는 경우
    if avg == subset_sum//N:
        avg = subset_sum//N

    print(f'#{tc} {avg}')
