#include <iostream>
#include <queue>

using namespace std;

int main() {

	int N, K;
	cin >> N >> K;
	queue<int> q;
	
	for (int i = 1; i <= N; i++)
		q.push(i);

	cout << "<";

	while (!q.empty()) {
		
		int temp = 0;

		if (q.size() == 1) {
			temp = q.front();
			q.pop();
			cout << temp;
			break;
		}

		for (int i = 0; i < K-1; i++) {

			temp = q.front();
			q.pop();
			q.push(temp);

		}

		temp = q.front();
		q.pop();
		cout << temp << "," << " ";

	}

	cout << ">";

}
