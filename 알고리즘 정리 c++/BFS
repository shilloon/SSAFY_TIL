#include <iostream>

int map[1001][1001] = { 0 }, visit[1001];
int q[1001];
int front = 0, rear = 0;

void bfs(int vertex) {

	visit[vertex] = 1;
	std::cout << vertex << std::endl;
	q[rear++] = vertex;
	while (front < rear) {
		vertex = q[front++];
		for (int i = 1; i <= n; i++) {
			if (map[vertex][i] == 1 && !visit[i]) {
				visit[i] = 1;
				std::cout << i << std::endl;
				q[rear++] = i;
			}
		}
	}

}
