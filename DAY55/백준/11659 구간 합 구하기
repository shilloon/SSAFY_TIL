#include <iostream>
#include <vector>

using namespace std;

int main() {

	ios::sync_with_stdio(false);
	cin.tie(NULL);

	int N, M;
	cin >> N >> M;

	vector<int> arr(N + 2, 0);
	for (int i = 1; i <= N; i++) {

		int num;
		cin >> num;
		arr[i] = arr[i-1] + num;

	}

	for (int i = 0; i < M; i++) {

		int result = 0;

		int a, b;
		cin >> a >> b;

		result = (arr[N] - arr[a - 1]) - (arr[N] - arr[b]);
		cout << result << "\n";

	}

	return 0;

}
