from collections import deque

for test_case in range(1, 11):

    T = int(input())

    password = list(map(int, input().split()))

    is_valid = True
    count = 0
    
    # password의 마지막 자리가 0보다 작거나 같을때 찾기
    while is_valid:

        count += 1
        password[0] -= count

        if count == 5:
            count = 0
        
        # 0보다 작을 경우 0으로 치기로 함
        if password[0] <= 0:
            password[0] = 0

        # 왼쪽 쉬프트
        password = deque(password)
        password.rotate(-1)
        password = list(password)
        
        # 마지막 자리가 0일 경우 비밀번호 완성
        if password[len(password)-1] == 0:
            is_valid = False
            break

    result = ' '.join(map(str, password))

    print(f'#{test_case} {result}')
